extends ../layout/base

block content 
    article.price-list 
        .price-list__container 
            .price-list__card.card 
                .card__img 
                    img(src="../src/static/img/card-img.jpg", class="card__pic")
                p.card__name Фанера ФК березовая (вторая строка)
                .card__characteristic 
                    span.card__characteristic-text Размер:
                    span.card__characteristic-value 3х1525х1525мм
                .card__characteristic 
                    span.card__characteristic-text Сорт:
                    span.card__characteristic-value IV/IV НШ
                p.card__in-stock <span class="card__in-stock-value">523</span> шт. на складе
                .card__bottom
                    .card__price
                        span.card__price-val <span class="card__price-num">1098</span> р
                        span.card__price-per-one за 1 шт 
                    .card__counter-block 
                        .card__counter 
                            a(class="card__minus-btn") -
                            input(type="number", class="card__field")
                            a(class="card__plus-btn") +
                        .card__shop-basket
                    a(href="#", class="card__clarify-remainder") уточнить остаток
